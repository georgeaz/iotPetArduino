void setup()
{
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay(); //for Clearing the display
  display.drawBitmap(0, 0, normalEye, 128, 64, WHITE);
  display.display();

  Wire.begin(8);                /* join i2c bus with address 8 */
  Wire.onReceive(receiveEvent); /* register receive event */
  Wire.onRequest(requestEvent); /* register request event */
  Serial.begin(9600);           /* start serial for debug */

  MP3.begin(9600);                               // Initiate the Serial MP3 Player Module.
  send_command_to_MP3_player(select_SD_card, 5); // Select the SD Card.

  pinMode(trigPin, OUTPUT);
  pinMode(echoPin, INPUT);

  pinMode(rPin, OUTPUT);
  pinMode(gPin, OUTPUT);
  pinMode(bPin, OUTPUT);

  myservo.attach(servoPin); // servo initialization
}

void loop()
{
  initDistanceSensor();
  //*************************    LDR sensor   ****************************
  /*  blue switch button is off
   *  red when theres light
   *  green when its dark
  */
  // read the state of the pushbutton value:
  buttonState = digitalRead(buttonPin);

  // check if the pushbutton is pressed. If it is, the buttonState is HIGH:
  if (buttonState == HIGH) {
    LDRFunction();
  }
  //************************* end of LDR sensor **************************
  else {
    lightBlue();
    if (distance < 3) {
      //light green
      lightMagenta();
      rotateServo();
    }
    else {
      myservo.write(0);
      //light blue
      lightBlue();
    }
    //**********************************************************************
  }

  //*************************************************************************

  if (sec == 20 && lock == 1) {
    current_command = "Play$        ";
    current_mood = "bored";
    petted_flag = 0;
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }
  if (sec == 40 && petted_flag != 1 && lock == 1) {
    // display sad face and move its ears:
    current_command = "Sad$         ";
    current_mood = "sad";
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }
  if (sec > 20 && sec <= 39 && petted_flag != 1 && distance < 3 && lock == 1) {
    // display happy face and move its ears
    current_command = "Happy$       ";
    current_mood = "happy";
    petted_flag = 1;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }
  
  if (sec == 45 && lock == 1) {
    current_command = "Sleep$       ";
    current_mood = "sleepy";
    sleep_flag = 0;
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 1;
    ldrValue = 10000;
    lock = 0;
  }
  if (sec == 65 && sleep_flag != 1 && lock == 1) {
    // display sad face
    current_command = "Sad$         ";
    current_mood = "sad";
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }
  if (sec > 45 && sec <= 64 && sleep_flag != 1 && ldrValue < 600 && lock == 1) {
    // display happy face and move its ears
    current_command = "Happy$       ";
    current_mood = "off";
    sleep_flag = 1;
    pet_is_sleeping = 1;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }
  
  if (sec == 70 && lock == 1) {
    // display black screen or sad face and move its ears
    current_command = "Hungry$      ";
    current_mood = "hungry";
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }
  
  if (sec == 85 && lock == 1) {
    // display black screen or sad face and move its ears
    current_command = "Thirsty$     ";
    current_mood = "thirsty";
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }

  if (sec == 100 && lock == 1) {
    // display sad face and move its ears
    current_command = "Dirty$       ";
    current_mood = "dirty";
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }

  if (sec == 105 && lock == 1) {
    // display sad face and move its ears
    current_command = "InLove$      ";
    current_mood = "inLove";
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }

  if(sec == 110 && level >= ADULT_LEVEL) {
    makeSound("Sentence1");
  }
  
  if (110 < sec && lock == 1) {
    current_command = "$            ";
    current_mood = "normal";
    turn_on_ldr_flag = 0;
    ldrValue = 10000;
    lock = 0;
  }

  //*******************************************************************************
  showCurrentReaction(current_mood);
  delay(1000);
  
  if (turn_on_ldr_flag == 1) {
    LDRFunction();
  }
  
  if(logged_user == 0) {
    duration = 0, distance = 0;
    ldrValue = 1000;
    petted_flag = 0;
    sleep_flag = 0;
    pet_is_sleeping = 0;
    turn_on_ldr_flag = 0;
    petType = "Dog";
    sec = 0;
    current_command = "$            ";
    current_mood = "normal";
    lock = 1;
  }
  if(logged_user == 1) {
    sec++;
  }
  if (sec == MAX_SEC) {
    current_command = "$            ";
    sec = 0;
  }
  Serial.println(sec);
  delay(1000);
}

// function that executes whenever data is received from master
void receiveEvent(int howMany) {
  String str = "";
  while (0 < Wire.available()) {
    char c = Wire.read(); /* receive byte as a character */
    Serial.print(c);      /* print the character */
    str += String(c);
    if(c == '#') {
      extractPetTypeAndLevel(str);    
    }
    if(c == '1') {
      logged_user = 1;
    }
    if(c == '0') {
      logged_user = 0;
    }
  }
  Serial.println(); /* to newline */
  //Serial.println("in receiveEvent: " + petType);
}

// function that executes whenever data is requested from master
void requestEvent() {
  if(current_command != "" && lock == 0) {
    Wire.write(current_command.c_str());
    lock = 1;
  }
}

//*********************** functions ************************************

void rotateServo() {
  for (pos = 0; pos <= 90; pos += 1) {
    myservo.write(pos);
    delay(15);
  }
  for (pos = 90; pos >= 0; pos -= 1) {
    myservo.write(pos);
    delay(15);
  }
}
void initDistanceSensor() {
  //************************* init distance sensor ****************************
  digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  //   
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  //   
  digitalWrite(trigPin, LOW);
  //     
  duration = pulseIn(echoPin, HIGH);
  // 
  distance = (duration / 2) / 29.1;
  //************************* end of init distance sensor **********************
}
void LDRFunction() {
  ldrValue = analogRead(ldrPin);
  //Serial.println(ldrValue);
  if (ldrValue > 600)
  {
    lightRed();
  }
  else
  {
    lightGreen();
  }
}

void lightMagenta() {
  analogWrite(rPin, 255);
  analogWrite(gPin, 0);
  analogWrite(bPin, 255);
}

void lightBlue() {
  analogWrite(rPin, 0);
  analogWrite(gPin, 0);
  analogWrite(bPin, 255);
}

void lightRed() {
  analogWrite(rPin, 255);
  analogWrite(gPin, 0);
  analogWrite(bPin, 0);
}

void lightGreen() {
  analogWrite(rPin, 0);
  analogWrite(gPin, 255);
  analogWrite(bPin, 0);
}

